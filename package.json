{
  "name": "b0x_portal",
  "version": "1.0.0",
  "description": "Frontend portal for b0x",
  "main": "portal.js",
  "scripts": {
    "test": "jest",
    "dev": "next",
    "build": "next build && next export -o dist",
    "lint": "eslint pages src",
    "lint:fix": "eslint pages src --fix",
    "prepush": "npm run lint && npm run test"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomcbean/b0x_portal.git"
  },
  "keywords": [],
  "author": "",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/tomcbean/b0x_portal/issues"
  },
  "homepage": "https://github.com/tomcbean/b0x_portal#readme",
  "dependencies": {
    "babel-plugin-styled-components": "^1.3.0",
    "next": "^4.2.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "styled-components": "^2.3.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.5.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-react": "^7.5.1",
    "husky": "^0.14.3",
    "jest": "^22.0.3",
    "prettier": "^1.9.2"
  }
}
