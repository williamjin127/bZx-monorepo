{
  "name": "bZx_portal",
  "version": "0.2.0",
  "description": "Frontend portal for bZx",
  "main": "portal.js",
  "scripts": {
    "test": "jest",
    "dev": "next",
    "build": "rimraf ./dist && next build && next export -o dist && rimraf ./live_site && node-mv ./dist ./live_site --parents",
    "prod": "cd dist && python -m http.server --bind 127.0.0.1",
    "lint": "eslint lib pages src",
    "lint:fix": "eslint lib pages src --fix",
    "prepush": "npm run lint"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomcbean/bZx_portal.git"
  },
  "keywords": [],
  "author": "",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/tomcbean/bZx_portal/issues"
  },
  "homepage": "https://github.com/tomcbean/bZx_portal#readme",
  "dependencies": {
    "0x.js": "^0.37.2",
    "bZx.js": "git+https://github.com/bZxNetwork/bZx.js.git",
    "babel-plugin-styled-components": "^1.5.1",
    "bignumber.js": "^7.0.1",
    "ethereum-blockies-png": "^0.1.2",
    "jss": "^9.8.1",
    "ledger-wallet-provider": "^2.0.1-beta.2",
    "material-ui": "^1.0.0-beta.45",
    "material-ui-pickers": "^1.0.0-rc.7",
    "moment": "^2.22.1",
    "next": "^6.0.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-jss": "^8.4.0",
    "styled-components": "^3.2.6",
    "web3": "^1.0.0-beta.34",
    "web3-provider-engine": "^14.0.6"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "node-mv": "^0.1.3",
    "prettier": "^1.12.1",
    "rimraf": "^2.6.2"
  }
}
