{
  "name": "b0x_portal",
  "version": "0.0.4",
  "description": "Frontend portal for b0x",
  "main": "portal.js",
  "scripts": {
    "test": "jest",
    "dev": "next",
    "build": "next build && next export -o dist",
    "prod": "cd dist && python -m http.server --bind 127.0.0.1",
    "lint": "eslint lib pages src",
    "lint:fix": "eslint lib pages src --fix",
    "prepush": "npm run lint"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomcbean/b0x_portal.git"
  },
  "keywords": [],
  "author": "",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/tomcbean/b0x_portal/issues"
  },
  "homepage": "https://github.com/tomcbean/b0x_portal#readme",
  "dependencies": {
    "0x.js": "^0.30.1",
    "b0x.js": "git+https://github.com/b0xNetwork/b0x.js.git",
    "babel-plugin-styled-components": "^1.3.0",
    "bignumber.js": "^6.0.0",
    "ethereum-blockies-png": "^0.1.2",
    "jss": "^9.5.0",
    "material-ui": "^1.0.0-beta.33",
    "material-ui-pickers": "^1.0.0-beta.15.1",
    "moment": "^2.20.1",
    "next": "^5.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-jss": "^8.2.0",
    "styled-components": "^2.3.3",
    "web3": "^1.0.0-beta.33"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.5.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-react": "^7.5.1",
    "husky": "^0.14.3",
    "jest": "^22.0.3",
    "prettier": "^1.9.2"
  }
}
